<h1>Bookings of <%= @flat.name %></h1>


<ul>
  <% @bookings.each do |booking| %>
  <li><%= booking.start_date %>|<%= booking.end_date %><%= link_to "delete", flat_booking_path(@flat,booking), method: :delete, data: { confirm: "Are u sure" } %></li>
  <% end %>
</ul>

<p><%= link_to "add a booking", flat_bookings_path(@flat) %></p>
